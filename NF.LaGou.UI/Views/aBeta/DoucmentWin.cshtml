@{
    ViewBag.Title = "DoucmentWin";
}
<script src="~/Scripts/jquery-1.8.2.js"></script>
<style>
    * {
        padding: 0;
        margin: 0;
        border: 0;
    }

    body {
        font-size: 14px;
        line-height: 1.5;
        background-color: white;
        font-family: "Hiragino Sans GB","Microsoft Yahei",SimSun,Arial,"Helvetica Neue",Helvetica;
    }

    ul {
        list-style-type: disc;
        -webkit-margin-after: 1em;
        -webkit-margin-start: 0px;
        -webkit-margin-end: 0px;
    }

    li {
        display: list-item;
        list-style: none;
        text-align: -webkit-match-parent;
    }

    h3 {
        display: block;
        -webkit-margin-before: 1em;
        -webkit-margin-after: 1em;
        -webkit-margin-start: 0px;
        -webkit-margin-end: 0px;
    }

    textarea {
        font-family: monospace;
        border-color: rgb(169, 169, 169);
    }

    #fireChat {
        display: none;
    }

    .hreader {
        height: 46px;
        line-height: 46px;
        color: #fff;
        text-align: center;
        background: url(/yipaiimage/logo_9c7dcca.png) 20px 12px no-repeat #00b38a;
        background-size: 60px 22px;
        border-top-left-radius: 3px;
        border-top-right-radius: 3px;
        position: relative;
    }

        .hreader .btns {
            padding: 15px 0;
            height: 16px;
            display: block;
            overflow: hidden;
            right: 0;
            top: 0;
            position: absolute;
        }

    .icon-base {
        margin: 0 16px 0 0;
        width: 16px;
        height: 16px;
        background-size: 16px 16px;
        vertical-align: top;
        cursor: pointer;
        display: inline-block;
    }

    .min {
        background: url(/yipaiimage/minimize_d3a54c4.png);
    }

    .close {
        background: url(/yipaiimage/close_aa55638.png);
    }

    .content {
        margin: 0 0 2px;
        border-top-width: 0;
        border: 1px solid #e8e8e8;
    }

    .chat .scroll-wrapper {
        width: 100%;
        height: 298px;
    }

    .mCustomScroll {
        touch-action: pinch-zoom;
    }

    .mCustomScrollBox {
        position: relative;
        overflow: hidden;
        height: 100%;
        max-width: 100%;
        outline: 0;
        direction: ltr;
    }

    .mCSB_inside > .mCSB-container {
        margin-right: 20px;
    }

    .mCSB-container {
        overflow: hidden;
        width: auto;
        height: auto;
    }

    .chat .chat-list {
        padding: 0 10px 0 20px;
        overflow-y: visible;
        list-style: none;
    }

        .chat .chat-list .left {
            text-align: left;
        }

        .chat .chat-list .chat-item {
            margin: 10px 0;
            width: 100%;
            font-size: 14px;
            line-height: 1.5;
            text-align: justify;
        }

    .chat .title {
        margin: 6px 0;
        color: #666;
        font-size: 12px;
    }

    .content .title {
        margin: 0;
        padding: 0;
        line-height: 1;
        color: #333;
        font-size: 14px;
        font-weight: 400;
    }

    .chat .chat-list .lg {
        color: #333333;
        background-color: #f4f4f4;
    }

    .chat .chat-list p {
        padding: 8px 10px;
        max-width: 240px;
        border-radius: 6px;
        display: inline-block;
        position: relative;
    }

    .chat .chat-list .system {
        line-height: 1.5;
        color: #d2d2d2;
        font-size: 12px;
        text-align: center;
    }

    .mCSB_scrollTools {
        opacity: .75;
        position: absolute;
        width: 16px;
        height: auto;
        left: auto;
        top: 0;
        right: 0;
        bottom: 0;
    }

        .mCSB_scrollTools .mCSB_draggerContainer {
            position: absolute;
            top: 0;
            left: 0;
            bottom: 0;
            right: 0;
            height: auto;
        }

        .mCSB_scrollTools .mCSB_dragger {
            cursor: pointer;
            width: 100%;
            height: 30px;
            z-index: 1;
        }

            .mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar {
                background-color: rgba(0,0,0,.4);
                position: relative;
                width: 4px;
                height: 100%;
                margin: 0 auto;
                -webkit-border-radius: 16px;
                -moz-border-radius: 16px;
                border-radius: 16px;
                text-align: center;
            }

        .mCSB_scrollTools .mCSB_draggerRail {
            background-color: rgba(255,255,255,.75);
            width: 2px;
            height: 100%;
            margin: 0 auto;
            -webkit-border-radius: 16px;
            -moz-border-radius: 16px;
            border-radius: 16px;
        }

    .chat .toolbox {
        margin: 40px 0 0;
        border-top: 1px solid #e8e8e8;
        position: relative;
    }

        .chat .toolbox .emoji {
            background: url(/yipaiimage/face-icon.png) center center no-repeat #999;
            background-size: 22px 20px;
        }

        .chat .toolbox .icon-base {
            margin: 9px 0 9px 9px;
            width: 22px;
            height: 20px;
            background-color: #999;
        }

            .chat .toolbox .icon-base:hover {
                background-color: #00b38a;
            }

        .chat .toolbox ul {
            width: 100%;
            background-color: #fff;
            z-index: 1;
            left: 0;
            position: absolute;
        }

            .chat .toolbox ul li {
                margin: 4px 2px;
                width: 25px;
                height: 25px;
                display: inline-block;
            }

        .chat .toolbox .text {
            margin: 9px 20px 0 0;
            color: #666;
            line-height: 20px;
            cursor: pointer;
            float: right;
        }

    .chat .char-text {
        margin: 0;
        padding: 10px 8px;
        width: 100%;
        height: 64px;
        font-size: 12px;
        line-height: 1.5;
        border-width: 0;
        box-sizing: border-box;
        overflow: hidden;
    }

    .chat .submit {
        float: right;
        color: #999999;
    }

        .chat .submit .submit-msg {
            margin: 0 4px 0 12px;
            padding: 12px 24px;
            height: 100%;
            color: #fff;
            background-color: #00b38a;
            border-radius: 2px;
            display: inline-block;
            cursor: pointer;
        }
</style>
	</head>
<body>
    <span id="fireChat">初始化聊天窗口</span>
    <div class="hreader">
        <span class="connect" style="display: none;">连接中...</span>
